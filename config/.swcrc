{
  "jsc": {
    "transform": {
      "react": {
        "runtime": "automatic",
        "throwIfNamespace": true,
        "importSource": "@emotion/react"
      }
    },
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    }
  },
  "env": {
    "targets": ">= 1%, not dead",
    "mode": "usage",
    "coreJs": 3
  },
  "minify": true
}
